{
  "name": "nodejs-brief",
  "version": "1.0.0",
  "description": "Imagination test brief for NodeJS API",
  "main": "dist/index.js",
  "files": [
    "src/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js --ignore ./dist/cache/\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "validate": "node validate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schrembot/nodejs-brief.git"
  },
  "author": "Alex Schembri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schrembot/nodejs-brief/issues"
  },
  "homepage": "https://github.com/Schrembot/nodejs-brief#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.9",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^14.1.0",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.4.6",
    "concurrently": "^7.0.0",
    "get-routes": "^2.1.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
